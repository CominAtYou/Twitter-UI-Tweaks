{
    "name": "Twitter Font Switcher",
    "description": "Switch the font on Twitter to Segoe UI Variable Display",
    "version": "1.4.3",
    "manifest_version": 3,
    "content_scripts": [
        {
            "css": [
                "FontInject.css",
                "ChirpFont.css"
            ],
            "matches": [
                "https://twitter.com/*",
                "https://x.com/*",
                "https://mobile.twitter.com/*",
                "https://mobile.x.com/*"
            ]
        }
    ]
}
