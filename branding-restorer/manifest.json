{
    "name": "Twitter Branding Restorer",
    "description": "Restore the former Twitter branding on Twitter.",
    "version": "1.1.7",
    "manifest_version": 3,
    "content_scripts": [
        {
            "css": [
                "icon.css",
                "tweetButtons.css"
            ],
            "js": [
                "restore.js",
                "icon.js"
            ],
            "matches": [
                "https://twitter.com/*",
                "https://x.com/*",
                "https://mobile.twitter.com/*",
                "https://mobile.x.com/*"
            ]
        }
    ]
}
