{
    "name": "Twitter Branding Restorer",
    "description": "Restore the former Twitter branding on Twitter.",
    "version": "1.0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "css": [
                "icon.css"
            ],
            "js": [
                "restore.js"
            ],
            "matches": [
                "https://twitter.com/*"
            ]
        }
    ]
}
